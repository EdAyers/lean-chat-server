{
  "importMap": "./import_map.json",
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any" ]
    }
  },
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictPropertyInitialization": false
  },
  "tasks": {
    "start": "deno run --allow-env --allow-net --watch=src/ src/main.ts"
  }
}
